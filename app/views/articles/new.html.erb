<h1>New Article</h1>
<p>
  <%= form_for(@article) do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :body, "Content" %><br>
      <%= f.text_area :body %>
    </p>
    <p>
      <%= f.submit "Create Article" %>
    </p>
  <% end %>
</p>
