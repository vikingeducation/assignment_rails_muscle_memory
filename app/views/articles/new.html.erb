<form action="<%= articles_path %>" method="post">
  <lable>Title</lable>
  <input type="text" name="article[title]">
  <lable>Body</lable>
  <input type="body" name="article[body]">
  <input  type="hidden"
          name="authenticity_token"
          value="<%= form_authenticity_token %>">
  <input type="submit" value="Submit">
</form>
