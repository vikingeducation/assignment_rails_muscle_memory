<h1>All Articles!</h1>
<ul>
  <% @articles.each do |article| %>
    <li> <%= link_to "Article ID: #{article.id}  Title: #{article.title}", article_path(article.id) %> <%= link_to "Edit This Article", edit_article_path(article.id) %> <%= link_to "Delete This Article", article_path(article.id), method: "delete" %> </li>
  <% end %>
</ul>