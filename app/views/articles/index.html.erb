<ul>
  <% @articles.each do |article| %>
    <li>
      <p><%= link_to article.title, article_path(article.id) %></p>
      <p><%= link_to article.id, article_path(article.id) %> </p>
      <p><%= link_to "Edit", edit_article_path(article.id) %> </p>
      <p><%= link_to "Delete", article_path(article.id), method: "delete" %> </p>
    </li>

  <% end %>
</ul>