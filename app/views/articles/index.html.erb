
<h4>check this out<h4>
<h4>All Articles</h4>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
        <%= link_to article.title, article_path(article) %>
        <%= link_to "edit", edit_article_path(article) %>
        <%= link_to "delete", article_path(article), method: :delete %>
    </li>
  <% end %>
</ul>
